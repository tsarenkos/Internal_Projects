@model TaskTracker.Web.Models.MyTaskViewModel
@{
    ViewData["Title"] = "Теги";
}

@if (Model != null)
{
<div class="box-shadow">
    @if (Model.Tags != null)
    {
        for (int i = 0; i < Model.Tags.Count; i++)
        {
            <a href="/MyTask/Index?tagname=@Model.Tags[i].Name" target="blank">#@Model.Tags[i].Name</a>
        }
    }
</div>
<br />
}
